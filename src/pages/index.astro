  <!-- Script para el menú hamburguesa - JavaScript vanilla -->
  <script>
    document.addEventListener('DOMContentLoaded', function() {
      // Obtener elementos del menú
      const menuToggleBtns = document.querySelectorAll('.luminix-menu-toggle');
      const menuWrapper = document.querySelector('.luminix-menu-wrapper');
      const body = document.body;
      
      // Función para toggle del menú
      function toggleMenu() {
        if (menuWrapper && body) {
          menuWrapper.classList.toggle('luminix-body-visible');
          body.classList.toggle('luminix-body-visible');
        }
      }
      
      // Agregar event listeners a todos los botones hamburguesa
      menuToggleBtns.forEach(function(btn) {
        btn.addEventListener('click', function(e) {
          e.preventDefault();
          toggleMenu();
        });
      });
      
      // Cerrar menú al hacer click fuera
      if (menuWrapper) {
        menuWrapper.addEventListener('click', function(e) {
          if (e.target === menuWrapper) {
            toggleMenu();
          }
        });
      }
      
      // Cerrar menú al hacer click en un enlace del menú
      const menuLinks = document.querySelectorAll('.luminix-mobile-menu-list a');
      menuLinks.forEach(function(link) {
        link.addEventListener('click', function() {
          toggleMenu();
        });
      });
    });
  </script>

